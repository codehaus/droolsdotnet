package org.drools.test;

import java.lang.Integer;
import java.lang.Float;
import org.drools.CheeseEqual;

rule "assert"
	when
		i : Integer()
	then
		assertLogical(new CheeseEqual(i.toString(), i.intValue()));
end

rule "modify"
	no-loop true;
	when
		c : CheeseEqual(price == 2)
	then
		c.setPrice(c.getPrice() + 1);
		modify(c);
end