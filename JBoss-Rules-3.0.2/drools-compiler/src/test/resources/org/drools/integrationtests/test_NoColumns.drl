package org.drools.test

global java.util.List list

rule "no columns1"
    when
    	eval(true);
    then
    	list.add("fired1");
end    

rule "no columns2"
    when
    	eval(false);
    then
    	list.add("fired2");
end 

rule "no columns3"
    when
    	eval(true);
    	eval(1==1);
    then
    	list.add("fired3");
end  

 
rule "no columns4"
    when
    	eval(false);
    	eval(true);
    	eval(1==1);    	
    then
    	list.add("fired4");
end 